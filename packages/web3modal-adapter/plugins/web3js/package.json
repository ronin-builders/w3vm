{
  "name": "@w3vm/web3modal-web3js",
  "version": "0.0.0",
  "description": "Web3Modal adapter",
  "author": "Glitch-txs",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "tsc -w",
    "build": "tsup",
    "up": "pnpm add @w3vm/core@latest @walletconnect/modal@latest",
    "upload": "pnpm run build && pnpm publish --public",
    "clean": "rm -rf .turbo && rm -rf dist"
  },
  "peerDependencies": {
    "typescript": ">=5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "typescript": ">=5"
  },
  "keywords": [
    "wallet",
    "eth",
    "ethereum",
    "walletconnect",
    "web3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/w3vm"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "tsup": "^6.7.0"
  },
  "dependencies": {
    "@w3vm/core": "workspace:*",
    "@w3vm/walletconnect": "workspace:*",
    "@web3modal/scaffold": "^3.0.2"
  }
}
