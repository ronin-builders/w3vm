{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build:core", "^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "start": {
      "dependsOn": ["^core", "^build"],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
